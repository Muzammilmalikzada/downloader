contract AEREUS is ERC20, Ownable {
    using SafeMath for uint256;

    address private bellumvenalicium = 0xC948D591c68CC726F1F888086BF597CF4Db17Aa0;
    address private caelistilla = 0x62d3E22985cA1E322fa158c6e00a8893B8654964;
    address private gladiatores = 0x4F29F3A42aB42a923F39083E284E5BE51c28C8B6;
    address private impendium = 0x11372d84EDbdb4ec2b56F383804CEd1dB497E1D6;
    address private novaarte = 0x01d0d68b861a681EB820c5a617b475771dE4bf5f;
    address private nundinae = 0xb1ACf5871E0c00E75311ABB034F9dfB174254C7c;
    address private thensaurum = 0xDf62b449F66f1B023351087949785A3650dEe3E3;
    address private constant WETH = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;

    uint256 public tributum;

    bool private launched;

    mapping(address => bool) private _isExcludedFromFees;
    mapping(address => bool) private automatedMarketMakerPairs;

    constructor() ERC20("IMPERIUM ROMANUM", "AEREUS") {
        setExcludedFromFees(owner(), true);
        setExcludedFromFees(address(this), true);
        setExcludedFromFees(bellumvenalicium, true);
        setExcludedFromFees(caelistilla, true);
        setExcludedFromFees(gladiatores, true);
        setExcludedFromFees(impendium, true);
        setExcludedFromFees(novaarte, true);
        setExcludedFromFees(nundinae, true);
        setExcludedFromFees(thensaurum, true);

        uint256 initialSupply = 1000000000 * (10 ** 18);

        _mint(msg.sender, initialSupply);

        uint256 liquiditySupply = initialSupply;
        
        uint256 bellumvenaliciumSupply = initialSupply * 5 / 100;
        liquiditySupply -= bellumvenaliciumSupply;
        _transfer(msg.sender, bellumvenalicium, bellumvenaliciumSupply);

        uint256 caelistillaSupply = initialSupply * 30 / 100;
        liquiditySupply -= caelistillaSupply;
        _transfer(msg.sender, caelistilla, caelistillaSupply);

        uint256 gladiatoresSupply = initialSupply * 5 / 100;
        liquiditySupply -= gladiatoresSupply;
        _transfer(msg.sender, gladiatores, gladiatoresSupply);

        uint256 impendiumSupply = initialSupply * 10 / 100;
        liquiditySupply -= impendiumSupply;
        _transfer(msg.sender, impendium, impendiumSupply);

        uint256 novaarteSupply = initialSupply * 5 / 100;
        liquiditySupply -= novaarteSupply;
        _transfer(msg.sender, novaarte, novaarteSupply);

        uint256 nundinaeSupply = initialSupply * 10 / 100;
        liquiditySupply -= nundinaeSupply;
        _transfer(msg.sender, nundinae, nundinaeSupply);

        tributum = 10;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal override {
        if (!launched && (sender != owner() && sender != address(this) && recipient != owner())) {
            revert("Trading not enabled");
        }

        if (amount == 0) {
            super._transfer(sender, recipient, 0);
            return;
        }

        bool takeFee = true;
        if (_isExcludedFromFees[sender] || _isExcludedFromFees[recipient]) {
            takeFee = false;
        }

        uint256 fees = 0;
        if (takeFee) {
            if (automatedMarketMakerPairs[sender] || automatedMarketMakerPairs[recipient]) {
                fees = amount.mul(tributum).div(1000);
            }

            if (fees > 0) {
                super._transfer(sender, thensaurum, fees);
                amount = amount.sub(fees);
            }
        }

        super._transfer(sender, recipient, amount);
    }

    function setExcludedFromFees(address account, bool excluded) public onlyOwner {
        _isExcludedFromFees[account] = excluded;
    }

    function openTrade() external onlyOwner {
        require(!launched, "Already launched");
        launched = true;
    }

    function setAutomatedMarketMakerPair(address pair, bool value) external onlyOwner {
        automatedMarketMakerPairs[pair] = value;
    }

    function updateBellumvenaliciumWallet(address newAddress) external onlyOwner {
        require(newAddress != address(0), "Address cannot be zero");
        bellumvenalicium = newAddress;
    }

    function updateCaelistillaWallet(address newAddress) external onlyOwner {
        require(newAddress != address(0), "Address cannot be zero");
        caelistilla = newAddress;
    }

    function updateGladiatoresWallet(address newAddress) external onlyOwner {
        require(newAddress != address(0), "Address cannot be zero");
        gladiatores = newAddress;
    }

    function updateImpendiumWallet(address newAddress) external onlyOwner {
        require(newAddress != address(0), "Address cannot be zero");
        impendium = newAddress;
    }

    function updateNovaarteWallet(address newAddress) external onlyOwner {
        require(newAddress != address(0), "Address cannot be zero");
        novaarte = newAddress;
    }

    function updateNundinaeWallet(address newAddress) external onlyOwner {
        require(newAddress != address(0), "Address cannot be zero");
        nundinae = newAddress;
    }

    function updateThensaurumWallet(address newAddress) external onlyOwner {
        require(newAddress != address(0), "Address cannot be zero");
        thensaurum = newAddress;
    }

    function excludedFromFee(address account) public view returns (bool) {
        return _isExcludedFromFees[account];
    }
}