pragma solidity ^0.8.13;

import "./ERC20.sol";
import "./Ownable.sol";

contract FonzyToken is Ownable, ERC20 {

  constructor(uint256 _totalSupply) ERC20("Fonzy", "FONZY") {
    _mint(msg.sender, _totalSupply);
  }

  function burn(uint256 value) external {
    _burn(msg.sender, value);
  }
}
